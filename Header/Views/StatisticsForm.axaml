<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="Header.Views.StatisticsForm"
        x:DataType="vm:StatisticsViewModel"
        xmlns:vm="clr-namespace:Header.ViewModels"
        Title="AcquaintanceForm">

    <!-- Ты промахнулся с названием класса (ты написал StatisticsFormViewModel а в папке ViewModel ты создал класс StatisticsViewModel) -->
    <Window.DataContext>
        <vm:StatisticsViewModel />
    </Window.DataContext>

    <DataGrid Name="DisciplinesDataGrid" 
              ItemsSource="{Binding Std}"
              GridLinesVisibility="All"
              Background="AliceBlue"
              BorderBrush="Gray"
              BorderThickness="2"
              VerticalAlignment="Stretch"
              HorizontalAlignment="Stretch">
        <DataGrid.Columns>
            <DataGridTextColumn Header="Имя" Binding="{Binding Name}" />
        </DataGrid.Columns>
    </DataGrid>
</Window>